CREATE TABLE Steven_Passengers (
PassengerID NUMBER(4) PRIMARY KEY,
PassengerName VARCHAR2(50) NOT NULL,
TicketType VARCHAR(20) NOT NULL,
PaymentInformation VARCHAR(200) UNIQUE NOT NULL, -- Enforce uniqueness of payment information
ContactInformation VARCHAR(200) NOT NULL
);

-- steven_Passengers table
